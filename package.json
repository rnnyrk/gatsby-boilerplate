{
  "name": "gatsby-boilerplate-ts",
  "description": "Gatsby boilerplate with Typescript and Styled Components",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Ronny Rook <info@ronnyrook.nl>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx src/",
    "typecheck": "tsc"
  },
  "dependencies": {
    "gatsby": "2.32.3",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-create-client-paths": "2.10.0",
    "gatsby-plugin-manifest": "2.12.0",
    "gatsby-plugin-offline": "3.10.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sharp": "2.14.1",
    "gatsby-plugin-styled-components": "3.10.0",
    "gatsby-plugin-typescript": "2.12.0",
    "gatsby-source-filesystem": "2.11.0",
    "gatsby-transformer-sharp": "2.12.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.12.13",
    "@types/node": "14.14.25",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "babel-plugin-styled-components": "1.12.0",
    "dotenv": "8.2.0",
    "eslint": "7.19.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "gatsby-plugin-module-resolver": "1.0.3",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "tslint": "6.1.3",
    "typescript": "4.1.3"
  }
}
